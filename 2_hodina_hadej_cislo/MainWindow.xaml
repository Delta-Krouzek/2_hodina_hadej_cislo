<Window x:Class="_2_hodina_hadej_cislo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_2_hodina_hadej_cislo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="labelNadpis" Content="Hádej číslo" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="36"/>
        <TextBox x:Name="textBoxMyslim" HorizontalAlignment="Left" Height="31" Margin="10,63,0,0" TextWrapping="Wrap" Text="Sem napiš číslo" VerticalAlignment="Top" Width="173" FontSize="20"/>
        <Button x:Name="buttonOdeslat" Content="Odeslat" HorizontalAlignment="Left" Margin="108,99,0,0" VerticalAlignment="Top" Width="75" FontSize="20" Click="buttonOdeslat_Click"/>
        <Button x:Name="buttonReset" Content="Reset" HorizontalAlignment="Left" Margin="10,99,0,0" VerticalAlignment="Top" Width="93" Height="31" FontSize="20" Click="buttonReset_Click"/>

    </Grid>
</Window>
